{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-hooks"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": [2, "never"],
    // "react/jsx-indent": [2, 2],
    // "react/jsx-indent-props": [2, 2],
    "react-hooks/rules-of-hooks": "error",
  },
  // "extends": "eslint:recommended"
}